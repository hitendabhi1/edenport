  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme",
    "TwentyTwenty"
  ],
  "homepage": "https://wordpress.org/themes/twentytwenty/",
  "bugs": {
    "url": "https://core.trac.wordpress.org/"
  },
  "devDependencies": {
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/scripts": "^16.1.5",
    "autoprefixer": "^10.4.5",
    "concurrently": "^6.5.1",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "rtlcss": "^3.5.0",
    "stylelint-a11y": "^1.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "rtlcssConfig": {
